@page
@using Entities
@model IndexModel
@{
    ViewData["Title"] = "Home page";
    List<Offer> offersList = Model.GetList();
}

<div class="text-center">

    <table style="padding: 10px; background-color: lightgray; border: 1px solid black;">

    @foreach(var offer in offersList) {
        <tr>
            <td> @offer.LogoLink </td>
            <td> @offer.Title </td>
            <td> @offer.CompanyName </td>
        </tr>
    }
    </table>

    <script>
        console.log("cokolwiek");
    </script>

</div>
